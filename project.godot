[application]

config/name="World Maker"
config/description="A tool for world building and writting"
run/main_scene="res://systems/OpenLastProject.tscn"
boot_splash/image="res://transparent image.png"
boot_splash/bg_color=Color( 0.141176, 0.141176, 0.141176, 0 )
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"

[autoload]

SystemSettings="*res://singletone/EditorSetting.tscn"
CommandSystem="*res://singletone/CommandSystem.tscn"
SystemDataManager="*res://singletone/system_data_manager.gd"
ProjectSettingsManager="*res://singletone/ProjectSetting.tscn"
LoadingSystem="*res://singletone/loading_system.gd"
Logger="*res://singletone/Logger.tscn"
GlobalActions="*res://singletone/global_actions.gd"
EventBus="*res://singletone/event_bus.gd"

[debug]

gdscript/completion/autocomplete_setters_and_getters=true

[display]

window/size/width=1920
window/size/height=1080
window/vsync/use_vsync=false

[editor_plugins]

enabled=PoolStringArray( "res://addons/gut/plugin.cfg" )

[input]

fullscreen={
